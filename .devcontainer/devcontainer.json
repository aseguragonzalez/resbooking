{
  "containerEnv": {
    "XDEBUG_MODE": "coverage,debug,develop"
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "DavidAnson.vscode-markdownlint",
        "dbaeumer.vscode-eslint",
        "MehediDracula.php-namespace-resolver",
        "recca0120.vscode-phpunit",
        "xdebug.php-debug",
        "redhat.vscode-xml",
        "wongjn.php-sniffer"
      ]
    }
  },
  "features": {
    "ghcr.io/gvatsal60/dev-container-features/pre-commit:1": {}
  },
  "image": "mcr.microsoft.com/devcontainers/php:8.4-bookworm",
  "name": "PHP 8.4",
  "remoteUser": "vscode"
}
